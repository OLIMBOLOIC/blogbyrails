<div id="page_wrapper">
    <h1>Nouveau Post</h1>

    <%= form_for :post, url: posts_path do |f| %>

        <p>
            <%= f.label :Titre %> <br>
            <%= f.text_field :titre %> <br>
        </p>
        <p>
            <%= f.label :Publication %> <br>
            <%= f.text_area :contenu %> <br>
        </p>
    <p>
            <%= f.submit :Envoyer %> <br>
        </p>
    <% end %>
</div>
